#include <stdio.h>
int main(){
    int i,j,k;
    int n=5;
    for(i=n;i>=1;i--){
        if(i%2!=0){
            for(j=1;j<=i;j++){
                printf("%d",j);
            }
        }
        else{
             for(k=i;k>=1;k--){
                printf("%d",k);
            }
        }
        printf("\n");
    }
} 
